<%- layout('layouts/boilerplate') %>

<div class="row">
    <div class="col-6 offset-3">
        <h1>To Do List</h1>

        <a href="/todolist/new">Add new todo</a>

        <ul>
            <% for(let todo of todos) {%>
            <li><a href="/todolist/<%= todo._id %>"> <%= todo.title %> </a></li>
            <% } %>
        </ul>
    </div>
</div>
